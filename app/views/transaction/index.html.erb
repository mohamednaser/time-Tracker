<div class="main-container">

    <%= render "layouts/left" %>
    <div class='right-container'>
        <div class="internal-header"> 
            <h2>All Transactions</h2>
        </div>
        <div class="total-credit-header"> 
            <h2>Total Logged hours </h2>
            <span> 45:12 hr<span>
        </div>
        <div class="transactios-cotainer">
            <%
                if @transactions.empty?
                        conat " <h2> NoTransactions </h2> "
                else 
                    @transactions.each do |transaction|
                        concat render "transaction", transaction: transaction             
                    end  
                end
                           
            %>
            <div class="new-transaction-div">
              <%= link_to  "New Transaction", transaction_new_path %>  
            </div>
        </div>
    </div>
</div>